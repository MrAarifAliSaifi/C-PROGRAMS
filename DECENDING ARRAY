#include<stdio.h>
int main()
{
	int a[10],i,j,temp=0;
	printf("PLEASE ENER THE ELEMENTS OF THE A ARRAY");
	for(i=0;i<10;i++)
	scanf("%d",&a[i]);
	for(i=0;i<10;i++)
	{
	  for(j=i+1;j<10;j++)
	  {
		if(a[i]<a[j])
		{
		temp=a[j];
		a[j]=a[i];
		a[i]=temp;
	    }
  	  }
	}
	for(i=0;i<10;i++)
	{
		printf("\t%d",a[i]);
	}
	
	
}
